<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		    
	    <record id="view_partner_bank_form" model="ir.ui.view">
	        <field name="name">view.partner.bank.form</field>
	        <field name="model">res.partner.bank</field>
	        <field name="inherit_id" ref="base.view_partner_bank_form"/>
	        <field name="arch" type="xml">
	            <xpath expr="//group[last()]" position="after">
	                <field name="lot_control_ids" >
	                	<tree editable="top">
	                		<!-- field name="display_name" / -->
	                		<field name="bank_id" invisible="0" />
	                		<field name="name" />
	                		<field name="cheque_number_from" />
	                		<field name="cheque_number_to" />
	                		<field name="next_number" />
	                		<button name="open_cheque_register" type="object"
	                			string="Cheque Registers" />
	                		<field name="user_id" />
	                		<field name="state" />
	                	</tree>
	                </field>
	            </xpath>
	        </field>
	    </record>
	   
    </data>
</openerp>