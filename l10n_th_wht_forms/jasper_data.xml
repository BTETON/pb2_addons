		<!-- Tax Receipt on Customer Invoice (กรณีคืนเงิน AV) -->
		<record id="withholding_cert" model="ir.actions.report.xml">
			<field name="string">Withholding CERT</field>
			<field name="auto">True</field>
			<field name="model">account.voucher</field>
			<field name="jasper_model_id" ref="account_voucher.model_account_voucher"/>
			<field name="name">Withholding CERT</field>
			<field name="report_name">withholding.cert</field>
			<field name="report_rml">l10n_th_wht_forms/jrxml_reports/withholding_cert.jrxml</field>
			<field name="menu">True</field>
			<field name="header">False</field>
			<field name="jasper_report">True</field>
			<field name="jasper_output">pdf</field>
		</record>
		<record id="withholding_cert_values" model="ir.values">
			<field name="name">Withholding CERT</field>
			<field name="model">account.voucher</field>
			<field name="key2" eval="'client_print_multi'"/>
			<field name="value_unpickle" eval="'ir.actions.report.xml, ' +str(withholding_cert)"/>
		</record>

	</data>
</openerp>
