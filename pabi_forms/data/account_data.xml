<?xml version="1.0"?>
<openerp>
	<data>

		<!-- delete existing reports -->
		<delete model="ir.actions.report.xml" search="[('id', '=', ref('account.account_invoices'))]" />

		<!-- หนังสือรับรองการหักเงินประกันผลงาน (supplier retention form)-->
		<record id="supplier_retention_form" model="ir.actions.report.xml">
			<field name="string">หนังสือรับรองการหักเงินประกันผลงาน</field>
			<field name="auto">True</field>
			<field name="model">account.invoice</field>
			<field name="jasper_model_id" ref="account.model_account_invoice"/>
			<field name="name">Supplier Retention Form</field>
			<field name="report_name">supplier.retention.form</field>
			<field name="report_rml">pabi_forms/jrxml_reports/account/supplier_retention_form.jrxml</field>
			<field name="menu">True</field>
			<field name="header">False</field>
			<field name="jasper_report">True</field>
			<field name="jasper_output">pdf</field>
			<field name="copies">1</field>
		</record>

		<record id="supplier_retention_form_values" model="ir.values">
			<field name="name">Supplier Retention Form</field>
			<field name="model">account.invoice</field>
			<field name="key2" eval="'client_print_multi'"/>
			<field name="value_unpickle" eval="'ir.actions.report.xml, ' +str(supplier_retention_form)"/>
		</record>

