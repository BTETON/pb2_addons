	<queryString>
		<![CDATA[SELECT pd.id as pd_id,
pd.schedule_date schedule_date,
(SELECT it.value FROM res_users ru
LEFT JOIN res_partner rp ON rp.id = ru.partner_id
LEFT JOIN hr_employee he ON he.employee_code = ru.login
LEFT JOIN hr_position hp ON hp.id = he.position_id
left join res_org ro on he.org_id = ro.id
LEFT JOIN ir_translation it ON it.res_id = ro.id AND it.name LIKE 'res.org,name'
WHERE ru.id = pd.user_id) as ou_name,
(SELECT search_key FROM res_partner
WHERE id = ou.partner_id) as ou_key,
pd.name as pd_number,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.request_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.request_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.request_uid) AND it.name LIKE 'hr.employee,last_name')
) as requested_by,
(SELECT hp.name FROM res_users ru
LEFT JOIN res_partner rp
ON rp.id = ru.partner_id
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
LEFT JOIN hr_position hp
ON hp.id = he.position_id
WHERE ru.id = pd.request_uid) as position,
/*(SELECT hd.name FROM res_users ru
LEFT JOIN res_partner rp
ON rp.id = ru.partner_id
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
LEFT JOIN hr_department hd
ON hd.id = he.department_id
WHERE ru.id = pd.request_uid) as department,
(SELECT ro.name FROM res_users ru
LEFT JOIN res_partner rp ON rp.id = ru.partner_id
LEFT JOIN hr_employee he ON he.employee_code = ru.login
LEFT JOIN hr_position hp ON hp.id = he.position_id
left join res_org ro on ro.id = he.org_id
WHERE ru.id = pd.request_uid) as department,*/
(SELECT rs.name FROM res_users ru
LEFT JOIN res_partner rp ON rp.id = ru.partner_id
LEFT JOIN hr_employee he ON he.employee_code = ru.login
LEFT JOIN hr_position hp ON hp.id = he.position_id
left join res_section rs on he.section_id = rs.id
WHERE ru.id = pd.request_uid) as department,
pd.objective,
pd.amount_total,
pd.delivery_address as deliver_to,
pm.name as method,
ppr.name as price_range,
pd.doc_body as footer,
(SELECT p.name FROM purchase_order p
WHERE p.requisition_id = pd.id AND p.state = 'confirmed') as rfq_number,
(SELECT CONCAT(COALESCE(rpt.name || ' ',''),rp.name) FROM res_partner rp
LEFT JOIN res_partner_title rpt
ON rpt.id = rp.title
WHERE rp.id = po.partner_id AND po.state = 'confirmed') as rfq_supplier,
(SELECT rp.taxbranch FROM res_partner rp
WHERE rp.id = po.partner_id AND po.state = 'confirmed') as rfq_taxbranch,
(SELECT p.amount_total FROM purchase_order p
WHERE p.requisition_id = pd.id AND p.state = 'confirmed') as rfq_amount_total,

po.minimum_planned_date-DATE(now()) as minimum_planned_date,
(SELECT p.amount_total_text_th FROM purchase_order p
WHERE p.requisition_id = pd.id AND p.state = 'confirmed'),

CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id
FROM res_users ru
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id
FROM res_users ru
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,last_name')
) as verified_by,
pd.date_verify as verified_date,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id
FROM res_users ru
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id
FROM res_users ru
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,last_name')
) as approver,
pd.date_doc_approve as date_of_approval,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.user_id) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id
FROM res_users ru
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id
FROM res_users ru
LEFT JOIN hr_employee he
ON he.employee_code = ru.login
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,last_name')
) as responsible,
pd.create_date as responsible_date,
pd.fine_num_days,
pd.date_fine,
pd.fine_condition

FROM purchase_requisition pd
LEFT JOIN operating_unit ou
ON ou.id = pd.operating_unit_id
LEFT JOIN purchase_method pm
ON pm.id = pd.purchase_method_id
LEFT JOIN purchase_price_range ppr
ON ppr.id = pd.purchase_price_range_id
LEFT JOIN purchase_order po
ON po.requisition_id = pd.id AND po.state = 'confirmed'




WHERE $X{IN, pd.id, PIDS}]]>
	</queryString>
