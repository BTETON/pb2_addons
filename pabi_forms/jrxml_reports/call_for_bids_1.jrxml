	<subDataset name="RFQ" uuid="f36f99ad-161b-40b3-bf6a-af1ab49eb7b4">
		<parameter name="ids" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
po.name as rfq_number,
CONCAT(COALESCE(rpt.name || ' ',''),rp.name) as supplier_name,
po.amount_total,
pd.fine_num_days,
pd.date_fine,
pd.fine_condition
FROM purchase_order po
LEFT JOIN res_partner rp
ON po.partner_id = rp.id
left join purchase_requisition pd on pd.id = po.requisition_id
LEFT JOIN res_partner_title rpt
ON rpt.id = rp.title
WHERE po.requisition_id = $P{ids}
AND po.order_type LIKE 'quotation']]>
		</queryString>
		<field name="rfq_number" class="java.lang.String"/>
		<field name="supplier_name" class="java.lang.String"/>
		<field name="amount_total" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Total]]></fieldDescription>
		</field>
		<field name="fine_num_days" class="java.lang.Integer">
			<fieldDescription><![CDATA[No. of Days]]></fieldDescription>
		</field>
		<field name="date_fine" class="java.sql.Date">
			<fieldDescription><![CDATA[Fine Date]]></fieldDescription>
		</field>
		<field name="fine_condition" class="java.lang.String">
			<fieldDescription><![CDATA[Fine Condition]]></fieldDescription>
		</field>
