	<subDataset name="pr_detail" uuid="c2b737ea-30d7-4d73-8452-d3402370e7b4">
		<parameter name="ids" class="java.lang.Integer">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString>
			<![CDATA[SELECT MIN(id) id,
TRIM(product_name) product_name,
price_unit,
SUBSTRING(string_agg(pr_no,',') FROM 1 for 10) pr_no,
SUM(product_qty) product_qty,
SUM(price_subtotal) price_subtotal,
unit
FROM
(SELECT pdl.id,
pdl.product_name product_name,
pdl.price_unit price_unit,
pr.name pr_no,
pdl.product_qty product_qty,
pdl.price_subtotal price_subtotal,
pu.name unit
FROM purchase_requisition_line pdl
LEFT JOIN purchase_request_purchase_requisition_line_rel prpd
ON prpd.purchase_requisition_line_id = pdl.id
LEFT JOIN purchase_request_line prl
ON prl.id = prpd.purchase_request_line_id
LEFT JOIN purchase_request pr
ON pr.id = prl.request_id
left join product_uom pu on pdl.product_uom_id = pu.id
WHERE pdl.requisition_id = $P{ids}
ORDER BY pdl.id ASC) as t
GROUP BY TRIM(product_name),unit,price_unit
ORDER BY id]]>
		</queryString>
		<field name="id" class="java.lang.Integer"/>
		<field name="product_name" class="java.lang.String">
			<fieldDescription><![CDATA[Supplier Product Name]]></fieldDescription>
		</field>
		<field name="price_unit" class="java.lang.Double">
			<fieldDescription><![CDATA[Unit Price]]></fieldDescription>
		</field>
		<field name="pr_no" class="java.lang.String"/>
		<field name="product_qty" class="java.math.BigDecimal"/>
		<field name="price_subtotal" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Amount]]></fieldDescription>
		</field>
		<field name="unit" class="java.lang.String"/>
		<variable name="COLUMN_SUM" class="java.lang.Integer" resetType="None" calculation="Count">
			<variableExpression><![CDATA[$V{COLUMN_COUNT}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="request_approval" uuid="8b340580-e27d-46c1-be2f-a07a69b3b3b6">
		<parameter name="ids" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
CONCAT(
COALESCE((SELECT it.value FROM ir_translation it
WHERE it.res_id = requested_employee.title_id AND it.name LIKE 'res.partner.title,name') || ' ',''),
(SELECT it.value FROM ir_translation it
WHERE it.res_id = requested_employee.id AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT it.value FROM ir_translation it
WHERE it.res_id = requested_employee.id AND it.name LIKE 'hr.employee,last_name')
) requested_by,
pd.create_date create_date,
CONCAT(
COALESCE((SELECT it.value FROM ir_translation it
WHERE it.res_id = approved_employee.title_id AND it.name LIKE 'res.partner.title,name') || ' ',''),
(SELECT it.value FROM ir_translation it
WHERE it.res_id = approved_employee.id AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT it.value FROM ir_translation it
WHERE it.res_id = approved_employee.id AND it.name LIKE 'hr.employee,last_name')
) approver,
pd.date_approve date_approve
FROM purchase_requisition pd
LEFT JOIN res_users requested
ON requested.id = pd.request_uid
LEFT JOIN hr_employee requested_employee
ON requested_employee.employee_code = requested.login
LEFT JOIN res_partner requested_name
ON requested_name.id = requested.partner_id
LEFT JOIN res_users approved
ON approved.id = pd.assign_uid
LEFT JOIN hr_employee approved_employee
ON approved_employee.employee_code = approved.login
LEFT JOIN res_partner approved_name
ON approved_name.id = approved.partner_id

WHERE pd.id = $P{ids}]]>
		</queryString>
		<field name="requested_by" class="java.lang.String">
			<fieldDescription><![CDATA[Requested by]]></fieldDescription>
		</field>
		<field name="create_date" class="java.sql.Timestamp">
			<fieldDescription><![CDATA[Created on]]></fieldDescription>
		</field>
		<field name="approver" class="java.lang.String"/>
		<field name="date_approve" class="java.sql.Date">
			<fieldDescription><![CDATA[Date Approved]]></fieldDescription>
		</field>
	</subDataset>
