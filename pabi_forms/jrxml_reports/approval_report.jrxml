		<field name="date_approve" class="java.sql.Date">
			<fieldDescription><![CDATA[Approved Date]]></fieldDescription>
		</field>
	</subDataset>
	<subDataset name="committee" uuid="2dbef4bc-1536-4f79-a4fe-b54d005ddf1a">
		<parameter name="ids" class="java.lang.Integer"/>
		<parameter name="method_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT prc.sequence, prc.name, prc.position
FROM purchase_requisition_committee prc
LEFT JOIN purchase_committee_type pct
ON pct.id = prc.committee_type_id
WHERE prc.requisition_id = $P{ids} and
CASE $P{method_id}
WHEN 3 THEN pct.code = '5'
WHEN 5 THEN pct.code = '2'
END
ORDER BY prc.sequence]]>
		</queryString>
