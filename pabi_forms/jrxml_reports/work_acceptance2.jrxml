	<subDataset name="po_committee">
		<parameter name="ids" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
poc.name,
poc.position
FROM purchase_order_committee poc
LEFT JOIN purchase_committee_type pct
ON pct.id = poc.committee_type_id
WHERE poc.order_id = $P{ids}
AND pct.id =
(SELECT DISTINCT
pct.id
FROM purchase_order_committee poc
LEFT JOIN purchase_committee_type pct
ON pct.id = poc.committee_type_id
WHERE poc.order_id = $P{ids}
AND (pct.id = 3 OR pct.id = 7) LIMIT 1)
ORDER BY poc.id]]>
		</queryString>
		<field name="name" class="java.lang.String"/>
		<field name="position" class="java.lang.String">
			<fieldDescription><![CDATA[Position]]></fieldDescription>
		</field>
	</subDataset>
	<parameter name="IDS" class="java.lang.Object"/>
	<parameter name="PIDS" class="java.util.List" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{IDS}.toList()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
rt.name tax_branch_name,
ou.name operation_unit_name,
pwa.name gr_no,
pwa.create_date create_date,
po.name po_name,
po.date_order po_date,
pwa.supplier_invoice invoice_no,
pwa.date_invoice invoice_date,
pwa.date_contract_start contract_start_date,
pwa.date_contract_end contract_end_date,
supplier.name supplier_name,
supplier.vat supplier_tax_id,
supplier.taxbranch supplier_taxbranch,
pwa.date_receive receive_date,
pwal.name acceptance_line_name,
pwal.to_receive_qty acceptance_line_qty,
CONCAT(pwal.to_receive_qty::integer,' ',pu.name) acceptance_line_uom,
pwa.eval_receiving deliver_score,
pwa.eval_quality quality_score,
pwa.eval_service service_score,
pol.price_unit acceptance_line_unit_price,
po.id po_id,
(pwa.date_receive-pwa.date_contract_end) contract,
pwa.is_manual_fine,
pwa.manual_fine,
((po.fine_rate/100)*po.amount_total) fine



FROM purchase_work_acceptance pwa
LEFT JOIN purchase_order po
ON po.id = pwa.order_id
LEFT JOIN res_taxbranch rt
ON po.taxbranch_id = rt.id
LEFT JOIN operating_unit ou
ON ou.id = po.operating_unit_id
LEFT JOIN res_partner rp
ON rp.id = ou.partner_id
LEFT JOIN res_partner supplier
ON supplier.id = po.partner_id
LEFT JOIN purchase_work_acceptance_line pwal
ON pwal.acceptance_id = pwa.id
LEFT JOIN product_product pp
ON pp.id = pwal.product_id
LEFT JOIN product_uom pu
ON pu.id = pwal.product_uom
LEFT JOIN purchase_order_line pol
ON pol.id = pwal.line_id
WHERE $X{IN,pwa.id,PIDS}]]>
	</queryString>
