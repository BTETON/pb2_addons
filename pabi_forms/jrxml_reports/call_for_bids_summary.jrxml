	<subDataset name="pr_detail" uuid="2e75cfb2-b673-44dc-997b-0d6e416246af">
		<parameter name="ids" class="java.lang.Integer">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="cfb_amount_total" class="java.lang.Number" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT apt.name da,
CONCAT((COALESCE(rpt.name || ' ','')),rp.name) as corp,
po.amount_total
FROM purchase_order po
left join account_payment_term apt on po.payment_term_id = apt.id
left join res_partner rp on po.partner_id = rp.id
LEFT JOIN res_partner_title rpt ON rpt.id = rp.title
WHERE requisition_id = $P{ids}]]>
		</queryString>
		<field name="da" class="java.lang.String"/>
		<field name="corp" class="java.lang.String"/>
		<field name="amount_total" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Total]]></fieldDescription>
		</field>
		<variable name="COLUMN_SUM" class="java.lang.Integer" resetType="None" calculation="Count">
			<variableExpression><![CDATA[$V{COLUMN_COUNT}]]></variableExpression>
		</variable>
		<variable name="difference" class="java.lang.Number">
			<variableExpression><![CDATA[$P{cfb_amount_total}-$F{amount_total}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="request_approval" uuid="aa971141-dab6-422f-8b75-0b81f595fd54">
		<parameter name="ids" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
(SELECT rp.name FROM res_partner rp
LEFT JOIN res_users ru
ON ru.partner_id = rp.id
WHERE ru.id = pr.requested_by) as requested_by,
pr.create_date,
(SELECT rp.name FROM res_partner rp
LEFT JOIN res_users ru
ON ru.partner_id = rp.id
WHERE ru.id = pr.requested_by) as approver,
pr.date_approve
FROM purchase_request pr
WHERE pr.id = $P{ids}]]>
		</queryString>
		<field name="requested_by" class="java.lang.String">
			<fieldDescription><![CDATA[Requested by]]></fieldDescription>
		</field>
		<field name="create_date" class="java.sql.Timestamp">
			<fieldDescription><![CDATA[Created on]]></fieldDescription>
		</field>
		<field name="approver" class="java.lang.String"/>
		<field name="date_approve" class="java.sql.Date">
			<fieldDescription><![CDATA[Approved Date]]></fieldDescription>
		</field>
	</subDataset>
	<subDataset name="committee" uuid="279c896d-f300-4bd4-9937-a5a207359791">
		<parameter name="ids" class="java.lang.Integer"/>
		<parameter name="method_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT prc.sequence, prc.name, prc.position
FROM purchase_requisition_committee prc
LEFT JOIN purchase_committee_type pct
ON pct.id = prc.committee_type_id
WHERE prc.requisition_id = $P{ids} and
CASE $P{method_id}
WHEN 3 THEN pct.code = '5'
WHEN 5 THEN pct.code = '2'
END
ORDER BY prc.sequence]]>
		</queryString>
		<field name="sequence" class="java.lang.Integer">
			<fieldDescription><![CDATA[Sequence]]></fieldDescription>
		</field>
		<field name="name" class="java.lang.String"/>
		<field name="position" class="java.lang.String">
			<fieldDescription><![CDATA[Position]]></fieldDescription>
		</field>
	</subDataset>
