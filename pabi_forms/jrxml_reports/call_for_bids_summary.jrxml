	<queryString>
		<![CDATA[SELECT
pd.id pd_id,

pt.name ty ,
pd.objective obj,
pd.description des,
(SELECT rs.name FROM res_section rs WHERE rs.id = pdl.section_id LIMIT 1),
(SELECT po.amount_total FROM purchase_order po WHERE po.requisition_id = pd.id AND po.state = 'done'),
pd.date_doc_approve,
pm.name method,
pd.doc_footer footer



FROM purchase_requisition pd
LEFT JOIN purchase_type pt
ON pt.id = pd.purchase_type_id
LEFT JOIN purchase_requisition_line pdl
ON pdl.requisition_id = pd.id
LEFT JOIN purchase_method pm
ON pm.id = pd.purchase_method_id
WHERE $X{IN,pd.id,PIDS}]]>
	</queryString>
