	</parameter>
	<queryString>
		<![CDATA[SELECT
pd.id pd_id,
pd.name pd_name,
CONCAT(
to_char(pd.schedule_date,'DD'),'/',
to_char(pd.schedule_date,'MM'),'/',
to_char(pd.schedule_date,'YYYY')
) pd_date,
CASE ou.code
WHEN 'สำนักงานกลาง' THEN ''
ELSE ou.code
END,
CONCAT(
to_char(current_date,'DD'),' ',
CASE to_char(current_date,'MM')
WHEN '01' THEN 'มกราคม'
WHEN '02' THEN 'กุมภาพันธ์'
WHEN '03' THEN 'มีนาคม'
WHEN '04' THEN 'เมษายน'
WHEN '05' THEN 'พฤษภาคม'
WHEN '06' THEN 'มิถุนายน'
WHEN '07' THEN 'กรกฎาคม'
WHEN '08' THEN 'สิงหาคม'
WHEN '09' THEN 'กันยายน'
WHEN '10' THEN 'ตุลาคม'
WHEN '11' THEN 'พฤศจิกายน'
WHEN '12' THEN 'ธันวาคม'
END,' ',
EXTRACT(year FROM current_date + interval '543 years')) now_date,
pt.name ty ,
pd.objective obj,
pd.description des,
(SELECT rs.name FROM res_section rs WHERE rs.id = (SELECT pdl.section_id FROM purchase_requisition_line pdl WHERE pdl.requisition_id = pd.id LIMIT 1)),
LTRIM((SELECT po.amount_total FROM purchase_order po WHERE po.requisition_id = pd.id AND po.state = 'done')::numeric::money::varchar, '฿ ')::varchar amount_total,
CONCAT(
to_char(pd.date_doc_approve ,'DD'),' ',
CASE to_char(pd.date_doc_approve ,'MM')
WHEN '01' THEN 'มกราคม'
WHEN '02' THEN 'กุมภาพันธ์'
WHEN '03' THEN 'มีนาคม'
WHEN '04' THEN 'เมษายน'
WHEN '05' THEN 'พฤษภาคม'
WHEN '06' THEN 'มิถุนายน'
WHEN '07' THEN 'กรกฎาคม'
WHEN '08' THEN 'สิงหาคม'
WHEN '09' THEN 'กันยายน'
WHEN '10' THEN 'ตุลาคม'
WHEN '11' THEN 'พฤศจิกายน'
WHEN '12' THEN 'ธันวาคม'
END,' ',
EXTRACT(year FROM pd.date_doc_approve  + interval '543 years')) datedocaprrove,
pm.name method,
pd.doc_body,
pd.doc_header,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.user_id) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,last_name')
) as responsible,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,last_name')
) as verified,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,last_name')
) approver,
pd.purchase_method_id

FROM purchase_requisition pd
LEFT JOIN purchase_type pt ON pt.id = pd.purchase_type_id
LEFT JOIN purchase_method pm ON pm.id = pd.purchase_method_id
left join res_users ru on pd.user_id = ru.id
left join res_partner rp on rp.id = ru.partner_id
left join res_users ru2 on pd.verify_uid = ru2.id
left join res_partner rp2 on rp2.id = ru2.partner_id
left join res_users ru3 on pd.doc_approve_uid = ru3.id
left join res_partner rp3 on rp3.id = ru3.partner_id
LEFT JOIN operating_unit ou ON ou.id = pd.operating_unit_id
WHERE $X{IN,pd.id,PIDS}]]>
	</queryString>
