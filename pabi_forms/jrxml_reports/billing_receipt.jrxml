	<queryString>
		<![CDATA[select
rt.name,
rt.street street_2,
concat(rt.street2,' ',rt.city,' ',rt.zip) street2_2,
rt.city city_2,
rt.zip zip_2,
rt.phone phone_2,
rt.fax fax_2,
ai.date_due,
ai.amount_untaxed,
ai.amount_tax,
ai.amount_total,
ai.reference po_no,
rp.name sup_name,
rp.phone sup_phone,
rp.fax sup_fax,
concat(rp.street,' ',rp.street2,' ',rp.city,' ',rp.zip) address,
ai.bill_note billnote,
billing_name.name billing_name,
ai.bill_number billnumber,
ai.date_bill datebill,
ai.date_invoice,
ai.supplier_invoice_number

from account_invoice ai
left join res_taxbranch rt on rt.id = ai.taxbranch_id
left join res_partner rp on ai.partner_id = rp.id
LEFT JOIN res_users billing
ON billing.id = ai.bill_uid
LEFT JOIN res_partner billing_name
ON billing_name.id = billing.partner_id
where $X{IN,ai.id,PIDS}]]>
	</queryString>
