	</parameter>
	<queryString>
		<![CDATA[SELECT
pb.id,
'-' as name,
'-' as address,
'-' as street_2,
'-' as street2_2,
'-' as phone_2,
'-' as fax_2,
supplier.name sup_name,
CONCAT(supplier.street,' ',supplier.street2,' ',supplier.city,' ',supplier.zip) street2_2,
supplier.phone sup_phone,
supplier.fax sup_fax,
pb.name billnumber,
pb.date datebill,
pb.date_due,
pb.amount_total,
'-' as billing_name,
'-' as billnote
FROM purchase_billing pb
LEFT JOIN res_partner supplier
ON supplier.id = pb.partner_id
WHERE $X{IN,pb.id,PIDS}]]>
	</queryString>
