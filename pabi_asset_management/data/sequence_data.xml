<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Asset Transfer -->
        <record id="seq_type_account_asset_transfer" model="ir.sequence.type">
            <field name="name">Asset Transfer</field>
            <field name="code">account.asset.transfer</field>
        </record>

        <record id="seq_account_asset_transfer" model="ir.sequence">
            <field name="name">Asset Transfer</field>
            <field name="code">account.asset.transfer</field>
            <field name="prefix">AST</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Asset Request -->
        <record id="seq_type_account_asset_request" model="ir.sequence.type">
            <field name="name">Asset Request</field>
            <field name="code">account.asset.request</field>
        </record>

        <record id="seq_account_asset_request" model="ir.sequence">
            <field name="name">Asset Request</field>
            <field name="code">account.asset.request</field>
            <field name="prefix">AI%(fy)s</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Asset Receive -->
        <record id="seq_type_account_asset_removal" model="ir.sequence.type">
            <field name="name">Asset Removal</field>
            <field name="code">account.asset.removal</field>
        </record>

        <record id="seq_account_asset_removal" model="ir.sequence">
            <field name="name">Asset Removal</field>
            <field name="code">account.asset.removal</field>
            <field name="prefix">AR%(fy)s</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
