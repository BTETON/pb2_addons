<?xml version="1.0" ?>
<openerp>
    <data>
		
    	# NSTDA Course
        <record id="view_nstda_course_tree" model="ir.ui.view">
            <field name="name">view.nstda.course.tree</field>
            <field name="model">nstda.course</field>
            <field name="arch" type="xml">
                <tree string="NSTDA Course">
                    <field name="name"/>
                    <field name="description" />
                </tree>
            </field>
        </record>
        
        <record id="view_nstda_course_form" model="ir.ui.view">
            <field name="name">view.nstda.course.form</field>
            <field name="model">nstda.course</field>
            <field name="arch" type="xml">
            	<form string="NSTDA Course">
                    <label for="name" class="oe_edit_only"/>
                    <h1>
                        <field name="name" class="oe_inline" placeholder="Name"/>
                    </h1>
                    <group>
                        <group>
	                        <field name="description" />
                        </group>
                        <group>
                        </group>
                    </group>
                </form>
            </field>
        </record>        

        <record id="action_nstda_course" model="ir.actions.act_window">
            <field name="name">NSTDA Course</field>
            <field name="res_model">nstda.course</field>
            <field name="view_type">form</field>
            <field name="view_id" ref="view_nstda_course_tree"/>
        </record>
        
        <menuitem name="NSTDA Course"
        	id="menu_nstda_course" 
        	action="action_nstda_course" 
        	parent="pabi_base.menu_spa_structure_tag" 
        	sequence="10"/>

	</data>
</openerp>
