<?xml version="1.0" ?>
<openerp>
    <data>

    	# Budgeting Level Config
        <record id="view_fiscalyear_budgeting_level_tree" model="ir.ui.view">
            <field name="name">view.fiscalyear.budgeting.level.tree</field>
            <field name="model">account.fiscalyear</field>
            <field name="arch" type="xml">
                <tree string="Fiscalyear Budgeting Level" editable="top">
                    <field name="name" readonly="1"/>
                    <field name="budgeting_level"/>
                </tree>
            </field>
        </record>

        <record id="action_fiscalyear_budgeting_level" model="ir.actions.act_window">
            <field name="name">Budgeting Level</field>
            <field name="res_model">account.fiscalyear</field>
            <field name="view_type">form</field>
            <field name="view_id" ref="view_fiscalyear_budgeting_level_tree"/>
        </record>
        <menuitem action="action_fiscalyear_budgeting_level" id="menu_fiscalyear_budgeting_level" parent="next_id_pos" sequence="110"/>

    </data>
</openerp>
